{
	"tokenColors": [
		{
			"settings": {
				"background": "#181818",
				"foreground": "#B9B9B9",
				"selectionBackground": "#3B3B3B",
				"activeGuide": "#777777"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Generic Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#70B433"
			}
		},
		{
			"name": "Quoted String",
			"scope": "string.quoted.docstring",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Special elements in doc comments",
			"scope": "storage.type.class.jsdoc",
			"settings": {
				"foreground": "#777777",
				"fontStyle": " bold"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#3FC5B7"
			}
		},
		{
			"name": "String interpolation and escaping",
			"scope": [
				"string.constant.character.escape",
				"string.interpolated.dollar.shell",
				"string.interpolated.backtick.shell",
				"constant.character.format",
				"constant.character.escape",
				"punctuation.section.embedded",
				"punctuation.definition.template-expression"
			],
			"settings": {
				"foreground": "#ED4A46"
			}
		},
		{
			"name": "Code embedded in string interpolation",
			"scope": "meta.embedded",
			"settings": {
				"foreground": "#B9B9B9"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"variable.other.constant",
				"constant.numeric",
				"constant.language"
			],
			"settings": {
				"foreground": "#3FC5B7"
			}
		},
		{
			"name": "Keyword Operator",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#70B433"
			}
		},
		{
			"name": "Generic Keywords",
			"scope": [
				"keyword.control",
				"keyword.other"
			],
			"settings": {
				"foreground": "#70B433"
			}
		},
		{
			"name": "Function and class defs",
			"scope": [
				"storage.type",
				"storage.type.function",
				"storage.type.class"
			],
			"settings": {
				"foreground": "#70B433"
			}
		},
		{
			"name": "Imports",
			"scope": [
				"keyword.control.import",
				"keyword.control.directive.include",
				"keyword.other.import"
			],
			"settings": {
				"foreground": "#E67F43"
			}
		},
		{
			"name": "Imported package and module names",
			"scope": [
				"storage.modifier",
				"storage.modifier.import.java"
			],
			"settings": {
				"foreground": "#B9B9B9"
			}
		},
		{
			"name": "Types and type modifiers",
			"scope": [
				"storage",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#DBB32D"
			}
		},
		{
			"name": "Keyword argument name in function call",
			"scope": "variable.parameter.function-call",
			"settings": {
				"foreground": "#B9B9B9"
			}
		},
		{
			"name": "Reserved variables like this, super, self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#ED4A46"
			}
		},
		{
			"name": "Built-ins, things from library and framework",
			"scope": "support",
			"settings": {
				"foreground": "#368AEB"
			}
		},
		{
			"name": "Exceptions",
			"scope": "support.type.exception",
			"settings": {
				"foreground": "#DBB32D"
			}
		},
		{
			"name": "Function decorator and similar elements",
			"scope": [
				"entity",
				"entity.name.function.decorator",
				"support.type.python"
			],
			"settings": {
				"foreground": "#368AEB"
			}
		},
		{
			"name": "Name of the defined class",
			"scope": [
				"entity.name.class",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#368AEB"
			}
		},
		{
			"name": "Name of the defined function",
			"scope": [
				"entity.name.function",
				"entity.name.function.declaration"
			],
			"settings": {
				"foreground": "#368AEB"
			}
		},
		{
			"name": "Function calls",
			"scope": [
				"meta",
				"meta.function-call.generic"
			],
			"settings": {
				"foreground": "#B9B9B9"
			}
		},
		{
			"name": "Function calls and defs use the same scope in JS",
			"scope": "entity.name.function.js",
			"settings": {
				"foreground": "#368AEB"
			}
		},
		{
			"name": "Ordinary variables",
			"scope": "variable",
			"settings": {
				"foreground": "#B9B9B9"
			}
		},
		{
			"name": "Parameter",
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#B9B9B9"
			}
		},
		{
			"name": "Shell builtins",
			"scope": "support.function.builtin.shell",
			"settings": {
				"foreground": "#70B433"
			}
		},
		{
			"name": "Special shell syntax",
			"scope": [
				"punctuation.definition.logical-expression",
				"variable.other.special.shell"
			],
			"settings": {
				"foreground": "#ED4A46"
			}
		},
		{
			"name": "Shell variable interpolation",
			"scope": [
				"variable.other.normal.shell",
				"variable.other.bracket.shell",
				"variable.other.positional.shell"
			],
			"settings": {
				"foreground": "#E67F43"
			}
		},
		{
			"name": "Token separators",
			"scope": [
				"punctuation",
				"punctuation.definition.block",
				"punctuation.definition.parameters",
				"punctuation.definition.arguments",
				"punctuation.definition.list",
				"punctuation.definition.dict",
				"punctuation.definition.binding-pattern",
				"punctuation.parenthesis",
				"punctuation.separator",
				"punctuation.accessor",
				"punctuation.section"
			],
			"settings": {
				"foreground": "#B9B9B9"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#FF5E56",
				"fontStyle": " bold"
			}
		},
		{
			"name": "Markup Heading",
			"scope": [
				"markup.heading",
				"meta.tag"
			],
			"settings": {
				"foreground": "#EFC541"
			}
		},
		{
			"name": "Markup Code Blocks",
			"scope": "support.function",
			"settings": {
				"foreground": "#368AEB"
			}
		},
		{
			"name": "Markup Bold",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": " bold"
			}
		},
		{
			"name": "Markup Italic",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": " italic"
			}
		},
		{
			"name": "CSS selectors and ids",
			"scope": [
				"entity.name.tag.css",
				"entity.other.attribute-name.id"
			],
			"settings": {
				"foreground": "#70B433"
			}
		},
		{
			"name": "HTML attributes",
			"scope": "entity.other.attribute-name.html",
			"settings": {
				"foreground": "#70B433"
			}
		}
	],
	"colors": {
		"editor.background": "#181818",
		"editorCursor.foreground": "#B9B9B9",
		"editor.foreground": "#B9B9B9",
		"editorWhitespace.foreground": "#252525",
		"editor.lineHighlightBackground": "#3B3B3B",
		"editor.selectionBackground": "#3B3B3B",
		"editorIndentGuide.background": "#3B3B3B"
	},
	"name": "Selenized Black"
}