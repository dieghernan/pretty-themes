{
  "name": "rOpenSpain",
  "author": "dieghernan",
  "semanticHighlighting": true,
  "type": "light",
  "tokenColors": [
    {
      "settings": {
        "background": "#F8F8F8",
        "foreground": "#000000"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#FF0000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "atrule",
      "scope": [
        "keyword.control.at-rule",
        "meta.at-rule",
        "support.constant.media.css"
      ],
      "settings": {
        "foreground": "#AA22FF"
      }
    },
    {
      "name": "attr-name",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#BB4444"
      }
    },
    {
      "name": "attr-value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html"
      ],
      "settings": {
        "foreground": "#BB4444"
      }
    },
    {
      "name": "bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "boolean",
      "scope": [
        "constant.language",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#AA22FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "class",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#0000FF",
        "fontStyle": "normal"
      }
    },
    {
      "name": "class-name",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#0000FF"
      }
    },
    {
      "name": "code",
      "scope": [
        "text.html.markdown markup.inline.raw",
        "text.html.markdown markup.raw"
      ],
      "settings": {
        "foreground": "#BB4444",
        "fontStyle": "normal"
      }
    },
    {
      "name": "comment",
      "scope": "comment",
      "settings": {
        "foreground": "#008800",
        "fontStyle": "italic"
      }
    },
    {
      "name": "content",
      "scope": "meta.embedded.line",
      "settings": {
        "foreground": "#BB6622"
      }
    },
    {
      "name": "constant",
      "scope": [
        "support.class.ruby",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#880000"
      }
    },
    {
      "name": "doctype",
      "scope": [
        "meta.tag.metadata.doctype",
        "meta.tag.sgml.doctype.html",
        "meta.tag.sgml.html"
      ],
      "settings": {
        "foreground": "#008800"
      }
    },
    {
      "name": "function",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#00A000"
      }
    },
    {
      "name": "important",
      "scope": [
        "constant.numeric.yaml-version",
        "entity.other.document",
        "keyword.other.directive",
        "keyword.other.important",
        "meta.directives",
        "punctuation.definition.alias",
        "punctuation.definition.anchor",
        "punctuation.section.embedded",
        "variable.other.alias",
        "variable.other.anchor"
      ],
      "settings": {
        "foreground": "#00BB00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "keyword",
      "scope": [
        "constant.language.undefined",
        "keyword",
        "markup.inline.raw",
        "markup.raw",
        "storage"
      ],
      "settings": {
        "foreground": "#AA22FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "keyword-null",
      "scope": "constant.language.null",
      "settings": {
        "foreground": "#AA22FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "namespace",
      "scope": [
        "entity.name.type.namespace",
        "storage.modifier.import"
      ],
      "settings": {
        "foreground": "#0000FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "number",
      "scope": [
        "constant.numeric",
        "constant.other.color",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#666666",
        "fontStyle": "normal"
      }
    },
    {
      "name": "operator",
      "scope": [
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.class",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.relational",
        "keyword.other",
        "punctuation.separator.dictionary.key-value"
      ],
      "settings": {
        "foreground": "#666666"
      }
    },
    {},
    {},
    {
      "name": "regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#BB6688"
      }
    },
    {
      "name": "selector",
      "scope": [
        "meta.selector",
        "source.css entity.name.tag",
        "source.css entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#008000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "string",
      "scope": "string",
      "settings": {
        "foreground": "#BB4444"
      }
    },
    {
      "name": "tag",
      "scope": [
        "meta.document.yaml",
        "meta.tag",
        "storage.type.tag"
      ],
      "settings": {
        "foreground": "#008000"
      }
    },
    {
      "name": "title",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#800080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "variable",
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#B8860B"
      }
    },
    {
      "name": "url",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#008000"
      }
    },
    {},
    {
      "name": "Scoped variable",
      "scope": [
        "source.css variable",
        "source.ruby variable.other.readwrite",
        "variable.language.this.php",
        "variable.other.php"
      ],
      "settings": {
        "foreground": "#B8860B"
      }
    },
    {
      "name": "Bolds",
      "scope": "entity.name.tag.html",
      "settings": {
        "foreground": "#008800",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown link strings",
      "scope": [
        "constant.other.reference.link",
        "markup.href",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#B8860B"
      }
    },
    {
      "name": "Markdown quotes",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#0044DD"
      }
    },
    {
      "name": "YAML directive",
      "scope": "storage.type.tag.verbatim.yaml",
      "settings": {
        "foreground": "#00BB00"
      }
    },
    {
      "name": "YAML rules",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#BB4444"
      }
    }
  ],
