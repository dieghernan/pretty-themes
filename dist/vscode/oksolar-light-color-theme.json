{
  "name": "OKSolar Light",
  "author": "dieghernan",
  "semanticHighlighting": true,
  "type": "light",
  "tokenColors": [
    {
      "settings": {
        "background": "#FBF7EF",
        "foreground": "#657377"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#8FAAAB"
      }
    },
    {
      "name": "Generic Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#819500"
      }
    },
    {
      "name": "Quoted String",
      "scope": "string.quoted.docstring",
      "settings": {
        "foreground": "#8FAAAB"
      }
    },
    {
      "name": "Special elements in doc comments",
      "scope": "storage.type.class.jsdoc",
      "settings": {
        "foreground": "#8FAAAB",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#259D94"
      }
    },
    {
      "name": "String interpolation and escaping",
      "scope": [
        "string.constant.character.escape",
        "string.interpolated.dollar.shell",
        "string.interpolated.backtick.shell",
        "constant.character.format",
        "constant.character.escape",
        "punctuation.section.embedded",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#F23749"
      }
    },
    {},
    {
      "name": "Constant",
      "scope": [
        "constant",
        "variable.other.constant",
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#259D94"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#819500"
      }
    },
    {
      "name": "Generic Keywords",
      "scope": [
        "keyword.control",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#819500"
      }
    },
    {
      "name": "Function and class defs",
      "scope": [
        "storage.type",
        "storage.type.function",
        "storage.type.class"
      ],
      "settings": {
        "foreground": "#819500"
      }
    },
    {
      "name": "Imports",
      "scope": [
        "keyword.control.import",
        "keyword.control.directive.include",
        "keyword.other.import"
      ],
      "settings": {
        "foreground": "#D56500"
      }
    },
    {},
    {
      "name": "Types and type modifiers",
      "scope": [
        "storage",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#AC8300"
      }
    },
    {},
    {
      "name": "Reserved variables like this, super, self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#F23749"
      }
    },
    {
      "name": "Built-ins, things from library and framework",
      "scope": "support",
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {
      "name": "Exceptions",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#AC8300"
      }
    },
    {
      "name": "Function decorator and similar elements",
      "scope": [
        "entity",
        "entity.name.function.decorator",
        "support.type.python"
      ],
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {
      "name": "Name of the defined class",
      "scope": [
        "entity.name.class",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {
      "name": "Name of the defined function",
      "scope": [
        "entity.name.function",
        "entity.name.function.declaration"
      ],
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {},
    {
      "name": "Function calls and defs use the same scope in JS",
      "scope": "entity.name.function.js",
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {},
    {},
    {
      "name": "Shell builtins",
      "scope": "support.function.builtin.shell",
      "settings": {
        "foreground": "#819500"
      }
    },
    {
      "name": "Special shell syntax",
      "scope": [
        "punctuation.definition.logical-expression",
        "variable.other.special.shell"
      ],
      "settings": {
        "foreground": "#F23749"
      }
    },
    {
      "name": "Shell variable interpolation",
      "scope": [
        "variable.other.normal.shell",
        "variable.other.bracket.shell",
        "variable.other.positional.shell"
      ],
      "settings": {
        "foreground": "#D56500"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#F23749",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Link",
      "scope": [
        "markup.link",
        "meta.link"
      ],
      "settings": {
        "foreground": "#259D94"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "markup.heading",
        "meta.tag",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#AC8300"
      }
    },
    {
      "name": "Other supports",
      "scope": "support.function",
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {
      "name": "Markup Code Blocks",
      "scope": [
        "markup.raw",
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {
      "name": "Markup Bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS selectors and ids",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#819500"
      }
    },
    {
      "name": "HTML attributes",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#819500"
      }
    },
    {
      "name": "YAML rules",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#819500"
      }
    },
    {
      "name": "YAML directive",
      "scope": "storage.type.tag.verbatim.yaml",
      "settings": {
        "foreground": "#2B90D8"
      }
    }
  ],
  "colors": {
