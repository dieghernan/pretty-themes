{
  "name": "GitHub Light",
  "author": "GitHub",
  "semanticHighlighting": true,
  "type": "light",
  "tokenColors": [
    {
      "settings": {
        "background": "#ffffff",
        "foreground": "#24292e"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#6A737D"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#005CC5"
      }
    },
    {
      "name": "Clojure workaround; don't highlight these separately from their enclosing scope",
      "scope": [
        "keyword.operator.symbole",
        "keyword.other.mark"
      ],
      "settings": {
        "foreground": ""
      }
    },
    {
      "name": "Entity",
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
        "foreground": "#6F42C1"
      }
    },
    {},
    {
      "name": {},
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#22863A"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#D73A49"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#D73A49"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string",
        "punctuation.section.embedded",
        "source"
      ],
      "settings": {
        "foreground": "#032F62"
      }
    },
    {
      "name": "Ada workaround; don't highlight imports as strings",
      "scope": "string.unquoted.import.ada",
      "settings": {
        "foreground": ""
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#005CC5"
      }
    },
    {
      "name": {},
      "scope": [
        "meta.property-name",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#005CC5"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#E36209"
      }
    },
    {
      "name": "Invalid - Broken",
      "scope": "invalid.broken",
      "settings": {
        "foreground": "#B31D28",
        "fontStyle": "bold italic underline"
      }
    },
    {
      "name": "Invalid – Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#B31D28",
        "fontStyle": "bold italic underline"
      }
    },
    {
      "name": "Invalid – Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#FAFBFC",
        "background": "#B31D28",
        "fontStyle": "italic underline"
      }
    },
    {
      "name": "Carriage Return",
      "scope": "carriage-return",
      "settings": {
        "foreground": "#FAFBFC",
        "background": "#D73A49",
        "fontStyle": "italic underline"
      }
    },
    {
      "name": "Invalid - Unimplemented",
      "scope": "invalid.unimplemented",
      "settings": {
        "foreground": "#B31D28",
        "fontStyle": "bold italic underline"
      }
    },
    {
      "name": {},
      "scope": [
        "message.error",
        "brackethighlighter.unmatched",
        "sublimelinter.mark.error"
      ],
      "settings": {
        "foreground": "#B31D28"
      }
    },
    {},
    {
      "name": "String variable",
      "scope": [
        "string",
        "variable"
      ],
      "settings": {
        "foreground": "#005CC5"
      }
    },
    {
      "name": "String.regexp",
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#032F62"
      }
    },
    {
      "name": "String.regexp.«special»",
      "scope": [
        "string.regexp.character-class",
        "string.regexp",
        "constant.character.escape",
        "source.ruby.embedded",
        "string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#032F62"
      }
    },
    {
      "name": "String.regexp constant.character.escape",
      "scope": [
        "string.regexp",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#22863A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Support.constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#005CC5"
      }
    },
    {
      "name": "Support.variable",
      "scope": "support.variable",
      "settings": {
        "foreground": "#005CC5"
      }
    },
    {
      "name": "meta module-reference",
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#005CC5"
      }
    },
    {
      "name": "Markup.list",
      "scope": "markup.list",
      "settings": {
        "foreground": "#735C0F"
      }
    },
    {
      "name": "Markup.heading",
      "scope": [
        "markup.heading",
        "entity.name"
      ],
      "settings": {
        "foreground": "#005CC5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup.quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#22863A"
      }
    },
    {
      "name": "Markup.italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup.bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup.raw",
      "scope": "markup.raw",
      "settings": {
        "foreground": "#005CC5"
      }
    },
    {
      "name": "Markup.deleted",
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "foreground": "#B31D28",
        "background": "#FFEEF0"
      }
    },
    {
      "name": "Markup.inserted",
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "foreground": "#22863A",
        "background": "#F0FFF4"
      }
    },
    {
      "name": {},
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "foreground": "#E36209",
        "background": "#FFEBDA"
      }
    },
    {
      "name": {},
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#F6F8FA",
        "background": "#005CC5"
      }
    },
    {
      "name": {},
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#6F42C1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Meta.separator",
      "scope": "meta.separator",
      "settings": {
        "foreground": "#005CC5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Output",
      "scope": "meta.output",
      "settings": {
        "foreground": "#005CC5"
      }
    },
    {
      "name": {},
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#586069"
      }
    },
    {
      "name": {},
      "scope": "sublimelinter.mark.warning",
      "settings": {
        "foreground": "#E36209"
      }
    },
    {
      "name": {},
      "scope": "sublimelinter.gutter-mark",
      "settings": {
        "foreground": "#959DA5"
      }
    },
    {
      "name": {},
      "scope": [
        "constant.other.reference.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#032F62",
        "fontStyle": "underline"
      }
    }
  ],
  "colors": {
    "activityBar.activeBackground": "#D3D4D5",
    "activityBar.background": "#FBFBFB",
    "activityBar.foreground": "#24292e",
    "activityBar.inactiveForeground": "#6A737D",
    "activityBarBadge.background": "#24292e",
    "activityBarBadge.foreground": "#ffffff",
    "badge.background": "#24292e",
    "badge.foreground": "#ffffff",
    "breadcrumb.activeSelectionForeground": "#24292e",
    "breadcrumb.background": "#E9EAEA",
    "breadcrumb.focusForeground": "#24292e",
    "breadcrumb.foreground": "#6A737D",
    "button.background": "#24292e",
    "button.foreground": "#ffffff",
    "button.secondaryBackground": "#FBFBFB",
    "button.secondaryForeground": "#24292e",
    "dropdown.background": "#E9EAEA",
    "dropdown.foreground": "#24292e",
    "editor.background": "#ffffff",
    "editor.foreground": "#24292e",
    "editor.inactiveSelectionBackground": "#fafbfc",
    "editor.lineHighlightBackground": "#fafbfc",
    "editor.lineHighlightBorder": "#c8c8fa",
    "editor.selectionBackground": "#c8c8fa",
    "editor.snippetFinalTabstopHighlightBackground": "#ffffff",
    "editor.snippetTabstopHighlightBackground": "#ffffff",
    "editor.snippetTabstopHighlightBorder": "#6A737D",
    "editorBracketHighlight.foreground1": "#24292e",
    "editorCodeLens.foreground": "#6A737D",
    "editorCursor.foreground": "#24292e",
    "editorGroupHeader.tabsBackground": "#FBFBFB",
    "editorHoverWidget.background": "#ffffff",
    "editorHoverWidget.border": "#6A737D",
    "editorIndentGuide.background": "#959da5",
    "editorLineNumber.foreground": "#24292e",
    "editorRuler.foreground": "#959da5",
    "editorSuggestWidget.foreground": "#24292e",
    "editorSuggestWidget.selectedBackground": "#c8c8fa",
    "editorWhitespace.foreground": "#959da5",
    "focusBorder": "#6A737D",
    "foreground": "#24292e",
    "gitDecoration.ignoredResourceForeground": "#6A737D",
    "input.background": "#E9EAEA",
    "input.foreground": "#24292e",
    "input.placeholderForeground": "#6A737D",
    "list.activeSelectionBackground": "#c8c8fa",
    "list.activeSelectionForeground": "#24292e",
    "list.dropBackground": "#c8c8fa",
    "list.hoverBackground": "#c8c8fa",
    "list.inactiveSelectionBackground": "#BDBFC0",
    "menu.background": "#FBFBFB",
    "panel.background": "#E9EAEA",
    "panelTitle.activeForeground": "#24292e",
    "panelTitle.inactiveForeground": "#6A737D",
    "peekView.border": "#c8c8fa",
    "peekViewEditor.background": "#ffffff",
    "peekViewResult.fileForeground": "#24292e",
    "peekViewResult.lineForeground": "#24292e",
    "peekViewResult.selectionBackground": "#c8c8fa",
    "peekViewResult.selectionForeground": "#24292e",
    "peekViewTitleDescription.foreground": "#6A737D",
    "peekViewTitleLabel.foreground": "#24292e",
    "progressBar.background": "#24292e",
    "scrollbarSlider.activeBackground": "#BDBFC0",
    "scrollbarSlider.background": "#D3D4D5",
    "settings.checkboxForeground": "#24292e",
    "settings.dropdownForeground": "#24292e",
    "settings.headerForeground": "#24292e",
    "settings.numberInputForeground": "#24292e",
    "settings.textInputForeground": "#24292e",
    "sideBar.background": "#E9EAEA",
    "sideBar.foreground": "#24292e",
    "sideBarSectionHeader.background": "#ffffff",
    "sideBarTitle.background": "#FBFBFB",
    "sideBarTitle.foreground": "#24292e",
    "statusBar.background": "#D3D4D5",
    "statusBar.foreground": "#24292e",
    "statusBar.noFolderBackground": "#c8c8fa",
    "statusBar.noFolderForeground": "#24292e",
    "statusBarItem.remoteForeground": "#ffffff",
    "tab.activeBackground": "#D3D4D5",
    "tab.activeForeground": "#24292e",
    "tab.inactiveBackground": "#E9EAEA",
    "tab.inactiveForeground": "#24292e",
    "terminal.background": "#ffffff",
    "terminal.foreground": "#24292e",
    "titleBar.activeBackground": "#FBFBFB",
    "titleBar.activeForeground": "#24292e",
    "titleBar.inactiveForeground": "#6A737D"
  }
}
