{
  "name": "StackOverflow Light",
  "author": "dieghernan",
  "semanticHighlighting": true,
  "type": "light",
  "tokenColors": [
    {
      "settings": {
        "background": "#f6f6f6",
        "foreground": "#2f3337"
      }
    },
    {
      "name": "Any comments",
      "scope": "comment",
      "settings": {
        "foreground": "#656E77"
      }
    },
    {
      "name": "Multi-line comments /* … */,<- … -->",
      "scope": "comment.block",
      "settings": {
        "foreground": "#656E77"
      }
    },
    {
      "name": "Line comments",
      "scope": "comment.line",
      "settings": {
        "foreground": "#656E77"
      }
    },
    {
      "name": "Others",
      "scope": "comment.line.number-sign",
      "settings": {
        "foreground": "#656E77"
      }
    },
    {
      "name": "Special constants like true, false,nil",
      "scope": "constant.language",
      "settings": {
        "foreground": "#B75501"
      }
    },
    {
      "name": "Any number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#B75501"
      }
    },
    {
      "name": "Floating number",
      "scope": "constant.numeric.float",
      "settings": {
        "foreground": "#B75501"
      }
    },
    {
      "name": "Integers",
      "scope": "constant.numeric.integer",
      "settings": {
        "foreground": "#B75501"
      }
    },
    {
      "name": "Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#54790D"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#B75501"
      }
    },
    {
      "name": "Tag name for CSS/SCSS",
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#015692"
      }
    },
    {
      "name": "ID of an attribute",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#B75501"
      }
    },
    {
      "name": "Name of an attribute (mainly in HTML)",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#015692"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#C02D2E"
      }
    },
    {
      "name": "Related to flow control",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#015692"
      }
    },
    {
      "name": "@media in css",
      "scope": "keyword.control.at-rule",
      "settings": {
        "foreground": "#015692"
      }
    },
    {
      "name": "Imports in some languages",
      "scope": "keyword.control.import",
      "settings": {
        "foreground": "#015692"
      }
    },
    {
      "name": "Important things",
      "scope": "keyword.other.important",
      "settings": {
        "foreground": "#015692"
      }
    },
    {
      "name": "YAML directives",
      "scope": "keyword.other.directive",
      "settings": {
        "foreground": "#015692"
      }
    },
    {
      "name": "Bold text",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Section header",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#015692"
      }
    },
    {
      "name": "Italic text",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup quotations",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#B75501"
      }
    },
    {
      "name": "Markup raw code",
      "scope": [
        "markup.raw",
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#535A60"
      }
    },
    {
      "name": "For DOCTYPE in HTML",
      "scope": "meta.doctype",
      "settings": {
        "foreground": "#656E77"
      }
    },
    {
      "name": "Type of something, class, function, int, var, etc.",
      "scope": "storage.type",
      "settings": {
        "foreground": "#015692"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#54790D"
      }
    },
    {
      "name": "Strings which are \"evaluated\"",
      "scope": "string.interpolated",
      "settings": {
        "foreground": "#54790D"
      }
    },
    {
      "name": "Quoted strings",
      "scope": "string.quoted",
      "settings": {
        "foreground": "#54790D"
      }
    },
    {
      "name": "Single quoted strings",
      "scope": "string.quoted.single",
      "settings": {
        "foreground": "#54790D"
      }
    },
    {
      "name": "Double quoted strings",
      "scope": "string.quoted.double",
      "settings": {
        "foreground": "#54790D"
      }
    },
    {
      "name": "Other types of quoting",
      "scope": "string.quoted.other",
      "settings": {
        "foreground": "#54790D"
      }
    },
    {
      "name": "Regular expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#54790D"
      }
    },
    {
      "name": "Strings in html",
      "scope": "string.html",
      "settings": {
        "foreground": "#54790D"
      }
    },
    {
      "name": "Types provided by the framework/library",
      "scope": "support.type",
      "settings": {
        "foreground": "#803378"
      }
    },
    {
      "name": "REVIEW! Types provided by the framework",
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#015692"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#54790D"
      }
    },
    {
      "name": "Reserved language variables like this,super,self, etc.",
      "scope": "variable.language",
      "settings": {
        "foreground": "#54790D"
      }
    },
    {
      "name": "Other variables",
      "scope": "variable.other",
      "settings": {
        "foreground": "#54790D"
      }
    },
    {
      "name": "CSS Types",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#803378"
      }
    },
    {
      "name": "Attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#015692"
      }
    },
    {
      "name": "Classes",
      "scope": "entity.name.type.class.php",
      "settings": {
        "foreground": "#B75501"
      }
    },
    {
      "name": "Markup Link",
      "scope": [
        "markup.link",
        "meta.link"
      ],
      "settings": {
        "foreground": "#54790D"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "punctuation.definition.list",
      "settings": {
        "foreground": "#535A60"
      }
    },
    {
      "name": "YAML rules",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#015692"
      }
    },
    {
      "name": "YAML directive",
      "scope": "storage.type.tag.verbatim.yaml",
      "settings": {
        "foreground": "#B75501"
      }
    }
  ],
  "colors": {
