{
  "tokenColors": [
    {
      "settings": {
        "background": "#1c1b1b",
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Any comments",
      "scope": "comment",
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Multi-line comments /* … */,<- … -->",
      "scope": "comment.block",
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Line comments",
      "scope": "comment.line",
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Others",
      "scope": "comment.line.number-sign",
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "Special constants like true, false,nil",
      "scope": "constant.language",
      "settings": {
        "foreground": "#f08d49"
      }
    },
    {
      "name": "Any number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#f08d49"
      }
    },
    {
      "name": "Floating number",
      "scope": "constant.numeric.float",
      "settings": {
        "foreground": "#f08d49"
      }
    },
    {
      "name": "Integers",
      "scope": "constant.numeric.integer",
      "settings": {
        "foreground": "#f08d49"
      }
    },
    {
      "name": "Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#b5bd68"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#f08d49"
      }
