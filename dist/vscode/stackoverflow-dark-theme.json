  "colors": {
    "editor.background": "#1c1b1b",
    "editorCursor.foreground": "#f08d49",
    "editor.foreground": "#ffffff",
    "editorWhitespace.foreground": "#272626",
    "editor.lineHighlightBackground": "#494949",
    "editor.selectionBackground": "#494949"
  },
  "name": "StackOverflow Dark",
  "author": "dieghernan"
}
