      "scope": "string.quoted.double",
      "settings": {
        "foreground": "#b5bd68"
      }
    },
    {
      "name": "Other types of quoting",
      "scope": "string.quoted.other",
      "settings": {
        "foreground": "#b5bd68"
      }
    },
    {
      "name": "Regular expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#b5bd68"
      }
    },
    {
      "name": "Strings in html",
      "scope": "string.html",
      "settings": {
        "foreground": "#b5bd68"
      }
    },
    {
      "name": "Types provided by the framework/library",
      "scope": "support.type",
      "settings": {
        "foreground": "#c59bc1"
      }
    },
    {
      "name": "REVIEW! Types provided by the framework",
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#88aece"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#b5bd68"
      }
    },
    {
      "name": "Reserved language variables like this,super,self, etc.",
      "scope": "variable.language",
      "settings": {
        "foreground": "#b5bd68"
      }
    },
    {
      "name": "Other variables",
      "scope": "variable.other",
      "settings": {
        "foreground": "#b5bd68"
      }
    },
    {
      "name": "CSS Types",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#c59bc1"
      }
    },
    {
