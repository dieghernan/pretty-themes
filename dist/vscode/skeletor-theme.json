    "editorLineNumber.foreground": "#655e7f",
    "editorSuggestWidget.foreground": "#ffffff",
    "editorSuggestWidget.selectedBackground": "#55535E",
    "editorWhitespace.foreground": "#363340",
    "extensionButton.prominentForeground": "#ffffff",
    "focusBorder": "#655e7f",
